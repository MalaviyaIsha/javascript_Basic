{
    "env": {
        "browser": true,
        "es2021": true
    },
    //"extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "semi": ["warn", "always"],
      "no-console": "off",
      "no-multi-spaces": "warn",
      "no-multiple-empty-lines": ["error", { "max": 1 }],
      "quotes": ["error", "double"],
      "max-lines": ["warn", { "max": 200 }]
    }
}
